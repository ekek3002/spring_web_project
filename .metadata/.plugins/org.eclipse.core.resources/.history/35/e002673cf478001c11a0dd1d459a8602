package org.zerock.domain;

import lombok.Getter;
import lombok.Setter;
import lombok.ToString;

@Getter
@Setter
@ToString
public class Criteria {

	private int pageName;
	private int amount;
	public Criteria() {
		this(1,10);
	}
	public Criteria(int pageName, int amount) {
		this.pageName = pageName;
		this.amount = amount;
	}
	
}
